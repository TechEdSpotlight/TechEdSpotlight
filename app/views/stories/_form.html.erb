<%= form_for(@story, html: { multipart: true }) do |f| %>
  <% if @story.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@story.errors.count, "error") %> prohibited this story from being saved:</h2>

      <ul>
      <% @story.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :overview %><br>
    <%= f.text_field :overview %>
  </div>
  <div class="field">
    <%= f.label :story %><br>
    <%= f.text_area :story %>
  </div>
  <div class="field">
    <%= f.label :source %><br>
    <%= f.text_field :source %>
  </div>
  <div class="field">
    <%= f.label :subject_list %><br>
    <%= f.text_field :subject_list %>
  </div>
  <div class="field">
    <%= f.label :computer_language_list %><br>
    <%= f.text_field :computer_language_list %>
  </div>
  <div class="field">
    <%= f.label :project_type_list %><br>
    <%= f.text_field :project_type_list %>
  </div>
  <div class="field">
    <%= f.label :attachment %><br>
    <%= f.file_field :attachment %>
  </div>
  <div class="actions">
    <%= f.submit 'Save' %>
    <%= f.submit 'Save and Publish' %>
  </div>
<% end %>
